<%- include('partials/header') %>
    <h1>Home</h1>
    <P>
        <%=startingContent%>
    </P>

    <% posts.forEach(post => {%>
        <h1> <%=post.tittle%> </h1>
        <p class=> <%=post.body.substring(0,100) + ' ...  '%>
            <% if(post.body.length>100){ %>
                <a href="posts/<%=post.tittle%>">Read More</a></p>
            <% } %>
            
        <% }) %>


